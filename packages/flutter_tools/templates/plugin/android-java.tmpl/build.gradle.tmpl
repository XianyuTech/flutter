group '{{androidIdentifier}}'
version '1.0'

buildscript {
    repositories {
        //集团mvn仓库地址
        maven { url "http://mvnrepo.alibaba-inc.com/mvn/repository" }
        google()
        jcenter()
    }

    dependencies {
        classpath 'com.android.tools.build:gradle:3.5.0'
    }
}

rootProject.allprojects {
    repositories {
        //集团mvn仓库地址
        maven { url "http://mvnrepo.alibaba-inc.com/mvn/repository" }
        maven { url 'http://flutter-storage.alibaba-inc.com/taobao'}
        //集团flutter镜像地址,修改系统变量FLUTTER_STORAGE_BASE_URL生效，参见文档：https://yuque.antfin-inc.com/aliflutter/bs13g7/fb1ghc
        maven { url '{{FLUTTER_STORAGE_BASE_URL}}'}
        google()
        jcenter()
    }
}

apply plugin: 'com.android.library'

android {
    compileSdkVersion 28

    defaultConfig {
        minSdkVersion 16
    }
    lintOptions {
        disable 'InvalidPackage'
    }
}
